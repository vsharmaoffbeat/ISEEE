<script type="text/javascript">

    $(document).ready(function () {
        $("#saveClassification").click(function (e) {
            if (parseInt($('#addmanufacture :selected').val()) <= 0 || parseInt($('#addSecondary :selected').val()) <= 0 || $('#datepicker6 input').val() == '') {
                alert("Please select Secondary Classification");
                return false;
            }
            else {

                //function SaveClassification() {
                //    txtTreatment
                //}
                data = { customerID: _customerId, requestDate: $('#datepicker6 input').val(), level2: parseInt($('#addSecondary :selected').val()), request: $('#txtRequest').val(), treatment: $('#txtTreatment').val(), treatmentDate: $('#datepicker5 input').val() }
                $.ajax({
                    type: "POST",
                    url: "/Customer/SaveClassificationRequestCustomerByDate",
                    data: data,
                    success: function (response) {
                        debugger;
                        if (response) {
                            alert("Save succesfully");
                            HideDialogR();
                        } else
                            alert("Failed to Save");

                    }
                })
            }

        });
        $("#btnShowSimple").click(function (e) {
            if (parseInt(_customerId) > 0) {
                GetAllStatesByCountry();
                bindTableRowClick();
                ShowDialog(false);
                e.preventDefault();
            }
            else
                alert('Must select customer');
        });

        $("#btnClose").click(function (e) {
            HideDialog();
            e.preventDefault();
        });
        $("#overlay").click(function (e) {
            return false;
        });


        $("#btnCancel").click(function (e) {
            HideDialog();
            e.preventDefault();
        });

        $("#addClassifications").click(function (e) {
            ShowDialogR(false);
            e.preventDefault();
        });

        $("#Closebtn").click(function (e) {
            HideDialogR();
            e.preventDefault();
        });



        $("#Closebtn").click(function (e) {
            HideDialogR();
            e.preventDefault();
        });
    });

    function ShowDialog(modal) {
        $("#overlay").show();
        $("#dialog").fadeIn(300);

        if (modal) {
            $("#overlay").unbind("click");
        }
        else {
            $("#overlay").click(function (e) {
                return false;
            });
        }
    }

    function HideDialog() {
        $("#overlay").hide();
        $("#dialog").fadeOut(300);
    }
    ;

    function ShowDialogR(modal) {
        $("#Rqstab").show();
        $("#Dialogtab").fadeIn(300);

        if (modal) {
            $("#Rqstab").unbind("click");
        }
        else {
            $("#Rqstab").click(function (e) {
                HideDialogR();
            });
        }
    }

    function HideDialogR() {
        $("#Rqstab").hide();
        $("#Dialogtab").fadeOut(300);
    }
</script>